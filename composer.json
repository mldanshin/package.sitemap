{
    "name": "mldanshin/package-sitemap",
    "description": "Danshin package sitemap",
    "license": "MIT",
    "authors": [
        {
            "name": "Maxim Danshin",
            "email": "mail@dahshin.net",
            "role": "Developer"
        }

    "require": {
        "php": "^8.3",
        "laravel/framework": "^11.0",
        "spatie/laravel-sitemap": "^7.2"
    },
    "require-dev": {
        "orchestra/testbench": "^7.0",
        "phpunit/phpunit": "^11.0",
        "phpstan/phpstan": "^1.3",
        "squizlabs/php_codesniffer": "^3.6"
    },
    "autoload": {
        "psr-4": {
            "Danshin\\Sitemap\\": "app/",
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Danshin\\Sitemap\\Providers\\PackegeServiceProvider"
            ]
        }
    }
}
